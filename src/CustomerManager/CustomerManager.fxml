<AnchorPane minHeight="750.0" minWidth="750.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="CustomerManager.CustomerManagerController">
   <children>
      <Label layoutX="81.0" layoutY="104.0" text="Mobile No.">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtMobile" layoutX="203.0" layoutY="100.0" onAction="#doShow" prefHeight="35.0" prefWidth="240.0" />
      <Label layoutX="93.0" layoutY="152.0" text="Name">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="203.0" layoutY="148.0" prefHeight="35.0" prefWidth="240.0" />
      <Label layoutX="86.0" layoutY="205.0" text="Address">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtAddress" layoutX="203.0" layoutY="201.0" prefHeight="35.0" prefWidth="240.0" />
      <Label layoutX="100.0" layoutY="254.0" text="Area">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtArea" layoutX="203.0" layoutY="250.0" prefHeight="35.0" prefWidth="240.0" />
      <ImageView fx:id="adminImg" fitHeight="197.0" fitWidth="276.0" layoutX="497.0" layoutY="91.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@download.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btnBrowse" layoutX="518.0" layoutY="309.0" mnemonicParsing="false" onAction="#doBrowse" prefHeight="31.0" prefWidth="115.0" style="-fx-font-weight: bold;" text="Browse" />
      <Label layoutX="102.0" layoutY="299.0" text="City">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtCity" layoutX="203.0" layoutY="295.0" prefHeight="35.0" prefWidth="240.0" />
      <Label layoutX="104.0" layoutY="317.0" />
      <Label layoutX="75.0" layoutY="342.0" text="Start Date">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <DatePicker fx:id="txtDate" layoutX="203.0" layoutY="339.0" prefHeight="35.0" prefWidth="240.0" />
      <Label layoutX="74.0" layoutY="389.0" text="Select Newspapers">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <ListView fx:id="lst1" layoutX="44.0" layoutY="426.0" onMouseClicked="#doSelectPrice" prefHeight="200.0" prefWidth="175.0" />
      <ListView fx:id="lst2" layoutX="233.0" layoutY="426.0" prefHeight="200.0" prefWidth="91.0" />
      <Button fx:id="btnGo" layoutX="337.0" layoutY="510.0" mnemonicParsing="false" onAction="#doGo" prefHeight="31.0" prefWidth="53.0" style="-fx-font-weight: bold;" text="GO" />
      <ListView fx:id="lst3" layoutX="405.0" layoutY="426.0" onMouseClicked="#dodelete" prefHeight="200.0" prefWidth="175.0" />
      <ListView fx:id="lst4" layoutX="597.0" layoutY="426.0" prefHeight="200.0" prefWidth="91.0" />
      <Label layoutX="405.0" layoutY="389.0" text="Selected Newspapers">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Button fx:id="btnSave" layoutX="74.0" layoutY="655.0" mnemonicParsing="false" onAction="#doSave" prefHeight="31.0" prefWidth="115.0" style="-fx-font-weight: bold;" text="Save" />
      <Button fx:id="btnStop" layoutX="249.0" layoutY="655.0" mnemonicParsing="false" onAction="#doStop" prefHeight="31.0" prefWidth="115.0" style="-fx-font-weight: bold;" text="Stop" />
      <Button fx:id="btnUpdate" layoutX="415.0" layoutY="655.0" mnemonicParsing="false" onAction="#doUpdate" prefHeight="31.0" prefWidth="115.0" style="-fx-font-weight: bold;" text="Update" />
      <Button fx:id="btnNew" layoutX="562.0" layoutY="655.0" mnemonicParsing="false" onAction="#doNew" prefHeight="31.0" prefWidth="115.0" style="-fx-font-weight: bold;" text="New" />
      <Label layoutX="225.0" layoutY="22.0" prefHeight="54.0" prefWidth="270.0" text="Costumer DashBoard" textFill="#973939">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
